<!-- color format: "normalColor|highlightedColor|pressedColor|disabledColor"  -->
<Defaults>
    <Button colors = "#FFFFFF00||#FFFFFF99|#FFFFFF00" onClick = "UIButtonClick"></Button>
    <Button class = "start_menu" colors = "#FFFFFF00||#FFFFFF99|#FFFFFF00" onClick = "UIButtonClick"></Button>
    <Button class = "travel_menu" active = "0"></Button>
</Defaults>

<Panel id = "start_menu_panel" width = "343" height = "341" visibility = "Host" active = "true">
    <Image id = "start_menu_background" image = "menu_background"></Image>
    <VerticalLayout id = "start_menu_buttons" spacing = "4" width = "243" height = "236">
        <Button class = "start_menu" id = "start_menu_adventure" icon = "menu_adventure"></Button>
        <Button class = "start_menu" id = "start_menu_traveling" icon = "menu_traveling"></Button>
        <Button class = "start_menu" id = "start_menu_settlements" icon = "menu_settlements"></Button>
        <Button class = "start_menu" id = "start_menu_guild" icon = "menu_guild"></Button>
        <Button class = "travel_menu" id = "travel_to_city" icon = "travel_to_city" active = "0"></Button>
        <Button class = "travel_menu" id = "travel_to_town" icon = "travel_to_town" active = "0"></Button>
        <Button class = "travel_menu" id = "travel_to_village" icon = "travel_to_village" active = "0"></Button>
    </VerticalLayout>
</Panel>
