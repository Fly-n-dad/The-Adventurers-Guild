<!-- color format: "normalColor|highlightedColor|pressedColor|disabledColor"  -->
<Defaults>
    <Panel class = "menu_background" width = "343" height = "341"></Panel>
    <Button colors = "#FFFFFF00|#FFFFFF99|#FFFFFFFF|#FFFFFF00" onClick = "UIButtonClick"></Button>
